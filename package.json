{
  "name": "techlead-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"next,api\" -c \"bgBlue,bgMagenta\" \"node server.js\" \"npm:start-api\"",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "start-api": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-ssr": "^4.0.0",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server-express": "^2.16.1",
    "babel-plugin-styled-components": "^1.11.1",
    "graphql": "^14.6.0",
    "isomorphic-unfetch": "^3.0.0",
    "json-server": "^0.16.1",
    "lodash": "^4.17.19",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
